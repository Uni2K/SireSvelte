{
  "name": "siresvelte",
  "version": "0.0.1",
  "scripts": {
    "dev:only": "svelte-kit dev",
    "build:only": "svelte-kit build",
    "preview": "svelte-kit preview",
    "tailwind:watch": "postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env tailwind mode=build cross-env node_env=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
    "build ": "npm run tailwind:build && npm run build:only"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^2.2.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "svelte-fa": "^2.2.1"

  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
